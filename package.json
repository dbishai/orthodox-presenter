{
  "name": "orthodox-presenter",
  "version": "0.0.1",
  "license": "GPL-3.0",
  "description": "Free and open-source web app for use in Coptic Orthodox church services",
  "repository": "https://github.com/dbishai/orthodox-presenter",
  "main": "js/app.js",
  "dependencies": {
    "babel-cli": ">=6.3.26",
    "classnames": "^2.1.3",
    "flux": ">=2.0.1",
    "keymirror": ">0.1.0",
    "object-assign": ">1.0.0",
    "react": ">=0.14.7",
    "react-dom": ">=0.14.7",
    "create-react-class": ">=15.5"
  },
  "devDependencies": {
    "browserify": ">=6.2.0",
    "envify": ">=3.0.0",
    "react-toggle": ">=2.0.1",
    "react-addons-shallow-compare": ">=0.14.7",
    "reactify": ">=0.15.2",
    "uglify-js": ">=2.4.15",
    "watchify": ">=2.1.1",
    "prop-types": ">=15.5.9"
  },
  "scripts": {
    "start": "watchify -o js/bundle.js -v -d js/app.js",
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > js/bundle.min.js"
  },
  "author": "David Bishai",
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  }
}
